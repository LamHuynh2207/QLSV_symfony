{% extends 'base.html.twig' %}

{% block title %}Mark{% endblock %}

{% block body %}
    <h1>Mark</h1>

    <table class="table  table-bordered container text-center px-5 ">
        <tbody>
            <tr>
                <th class="table-primary">Id</th>
                <td>{{ mark.id }}</td>
            </tr>
             <tr>
                <th class="table-info">Student</th>
                <td>{{ mark.Student.Name }}</td>
            </tr>
             <tr>
                <th class="table-primary">Subject</th>
                <td>{{ mark.Subject.Name }}</td>
            </tr>
            <tr>
                <th>ASM1</th>
                <td>{{ mark.ASM1 }}</td>
            </tr>
            <tr>
                <th>ASM2</th>
                <td>{{ mark.ASM2 }}</td>
            </tr>
            <tr>
                <th>Average</th>
                <td>{{ mark.Average }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ mark.Status }}</td>
            </tr>
        </tbody>
    </table>
     <div class="container px-5">
    <a href="{{ path('app_mark_index') }}">back to list</a>

    <a href="{{ path('app_mark_edit', {'id': mark.id}) }}">edit</a>

    {{ include('mark/_delete_form.html.twig') }}
    </div>
{% endblock %}

{{ form_start(form) }}
    {# {{ form_widget(form) }} #}
    <div class="container">
    <div class="row">
        <label>
            {{ form_label(form.Name, "Name Teacher") }}
        </label>
        {{ form_widget(form.Name, {'attr': {'class': 'form-control'}}) }}
    </div>
    <div class="row">
        <label>
            {{ form_label(form.Phone, "Phone") }}
        </label>
        {{ form_widget(form.Phone, {'attr': {'class': 'form-control'}}) }}
    </div>

    <div>
    <button class="btn btn-outline-info">{{ button_label|default('Save') }}</button>
    </div>

{{ form_end(form) }}
